{
    "extends": "@augu/tsconfig/dom.json",
    "exclude": ["node_modules"],
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "allowJs": true,
        "noEmit": true,
        "jsx": "preserve"
    }
}
