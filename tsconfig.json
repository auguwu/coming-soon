{
  "extends": "@augu/tsconfig/dom.json",
  "compilerOptions": {
    "resolveJsonModule": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "module": "ESNext",
    "noEmit": true,
    "jsx": "preserve"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
